<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <RootNamespace>Framework.Messages</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <SupportedPlatform Include="browser" />
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
    <InternalsVisibleTo Include="Framework.Messages.Dashboard.K8s" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Consul" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Framework.Messages.Core\Framework.Messages.Core.csproj" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="wwwroot/dist/**/*" Exclude="**/*/*.map" />
    <Compile
      Include="..\DotNetCore.CAP\Internal\ObjectMethodExecutor\*.cs"
      Link="ObjectMethodExecutor\%(Filename)%(Extension)"
    />
  </ItemGroup>
</Project>
